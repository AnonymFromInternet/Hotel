{{template "base".}}

{{define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1 class="text-center mt-3">Choose room</h1>

                {{$rooms := index .Data "rooms"}}
                {{range $rooms}}
                    {{.RoomName}}
                {{end}}
            </div>
        </div>
    </div>
{{end}}